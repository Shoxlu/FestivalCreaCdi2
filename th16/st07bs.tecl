void Revive()
{
	if(GF0 == -1f)
	{
		wait(200);
		//@charge() async;
		times(90)
		{
			enmCreateA("Petals1", 212f, RANDF*448f, 100, 100, 1);
			enmCreateA("Petals", -212f, RANDF*448f, 100, 100, 1);
			enmCreateA("Petals", 192f*RANDF2, -20f, 100, 100, 1);
			wait(2);
		}
		times(20)
		{
			anmPlay(1, 86);
			anmPlay(1, 90);
			wait(1);
		}
		playSound(SE_EXPLODE2);
		@Boss10();
	}
}
