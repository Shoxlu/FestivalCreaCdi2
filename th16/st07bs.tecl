void followBoss(float A)
{
	while(1)
	{
		if(GI0 != 0)
		{
			movePos(BOSS_X, BOSS_Y-A);
		}
		wait(1);
	}
}
void timeoutMode()
{
	flagSet(3);
	flagSet(4096);
	followBoss(30f) async;
	bombShield(0, 21);
	wait(100000);
}
void HitboxEnm()
{
	flagSet(2);
	flagSet(128);
	setHurtbox(384f, 50f);
	flagSet(4096);
	bombShield(1, 21);
	followBoss(30f) async;
	lifeSet(100000);
	int A = LIFE;
	while(1)
	{
		while(GI2 == 1)
		{
			while(GI3 == 1)
			{
				flagClear(1);
				if(A != LIFE)
				{
					if(SHOTTYPE == 2)
					{
						BI3 = BI3 + 2;
					}else{
						BI3 = BI3 + 5;
					}
					lifeSet(100000);
					A = LIFE;
				}
				wait(1);
			}
			flagSet(1);
			wait(1);
		}
		etOn(0);
		flagClear(128);
		flagClear(4096);
		delete();
	}
}
