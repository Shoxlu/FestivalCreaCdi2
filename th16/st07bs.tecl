void Boss1_at2(int color)
{
	float D = 1f;
	float F;
	float speed = 0.7f;
	float speed2 = 0.2f;
	float B = rad(120);
	float C;
	int rad;
	etNew(2);
	etAim(2, 1);
	etSprite(2, 22, color);
	I0 = color;
	etCount(2, 1, 1);
	etSpeed(2, 0.7f, 2f);
	if(RAND % 2)
	{
		etAngle(2, rad(45), rad(90));
		rad = 45;
	}else{
		etAngle(2, rad(135), rad(90));
		rad = 135;
	}
	etExSet(2, 0, 1, EX_ANIM, 0, NEG, NEGF, NEGF);
	etExSet(2, 1, 0, EX_VELTIME, 120, NEG, 0.1f, NEGF);
	//etExSet(2, 4, 0, EX_VELTIME, 60, NEG, 4f, NEGF);
